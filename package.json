{
	"name": "@jdeighan/create-app",
	"version": "2.0.28",
	"type": "module",
	"author": "John Deighan",
	"license": "MIT",
	"scripts": {
		"readme": "cp ./README.md ./src/routes/readme/+page.md",
		"coffee": "npx coffee -c --bare --no-header .",
		"coffee:watch": "npx coffee -c --bare --no-header -w .",
		"vite:dev": "vite dev",
		"dev": "npm run readme && npm run coffee && run-p coffee:watch vite:dev",
		"build": "npm run readme && npm run coffee && vite build",
		"preview": "vite preview"
	},
	"bin": {
		"@jdeighan/create-app": "./src/bin/install.js"
	},
	"devDependencies": {
		"@jdeighan/base-utils": "^4.0.24",
		"@jdeighan/svelte-utils": "^2.0.5",
		"@sveltejs/adapter-auto": "^2.0.1",
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/adapter-vercel": "^2.4.2",
		"@sveltejs/kit": "^1.15.9",
		"autoprefixer": "^10.4.14",
		"coffeescript": "^2.7.0",
		"mdsvex": "^0.10.6",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.14",
		"postcss-load-config": "^4.0.1",
		"postcss-preset-env": "^8.3.2",
		"svelte": "^3.54.0",
		"svelte-preprocess": "^5.0.3",
		"vite": "^4.3.3",
		"workbox-precaching": "^6.5.4"
	}
